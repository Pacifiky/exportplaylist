{
  "manifest_version": 2,
  "name": "ExportPlaylist",
  "version": "1.0",
  "description": "Export YouTube playlists!",
  "page_action": {
    "default_icon": {
      "16": "icon-16.png",
      "48": "icon-48.png",
      "128": "icon-128.png"
    }
  },
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "content_scripts": [
    {
      "matches": [ "https://www.youtube.com/playlist" ],
      "js": [ "script.js" ]
    }
  ]
}